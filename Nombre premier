package main;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        // write your code here
        System.out.println("Veuillez entrer le nombre");
        Scanner recuperation =new Scanner(System.in) ;
        int N = recuperation.nextInt() ;

        //test

        boolean c =true ;

        for(int i=2; i<N ;++i) {
            if(N%i==0){c=false ;}
        }
        if(c==true){System.out.println("C'est un nombre premier");}
        else {System.out.println("Ce n'est pas un nombre premier");}
        }
}
